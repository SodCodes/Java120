import java.util.*; //Import Packages
public class Main {
    static Scanner input = new Scanner(System.in);
    public static void main(String[] args)
    {
        String name;
        String password;
        int i =0;


        do
        {
            System.out.println("Please type username.");
            name = input.next();
            System.out.println("Please type in your password.");
            password = input.next();
        i++;
        } while (name.compareToIgnoreCase(password)==0 || password.length() < 12 && i < 3); // While loop, condition
    }
}
